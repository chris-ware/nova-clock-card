{
    "name": "chris-ware/nova-clock-card",
    "description": "A Laravel Nova card to display the date and time.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "ChrisWare\\NovaClockCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrisWare\\NovaClockCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
